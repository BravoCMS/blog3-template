<{extends 'layout/layout.tpl'}><{block name="template-name"}>    /photo_albums.tpl<{/block}><{block name=title}>    <{$module.name|htmlspecialchars}><{/block}><{block name="module"}>    <{$module.text}>    <div class="main_container">        <div id="main">            <div id="container">                <div id="out">                    <div id="in">                        <div class="main-content-wrp">                            <div id="center">                                <div class="albums-wrp">                                    <div class="albums-list">                                        <{foreach $photo_albums as $photo_album}>                                            <{if $photo_album.cover}>                                                <a href="<{$photo_album.relative_url}>" style="background: url(<{$photo_album.cover.photo_file_small}>) center center / cover no-repeat;">                                                    <img src="<{$photo_album.cover.photo_file_small}>" alt="pic" />                                                    <h3 style="color:#804D11;"><{$photo_album.name|escape}></h3>                                                </a>                                            <{/if}>                                               <{/foreach}>                                    </div>                                </div>                            </div>                        </div>                    </div>                </div>            </div>        </div>    </div>    <div class="row">        <{include 'misc/pagination.tpl'}>    </div><{/block}>